#%RAML 1.0 DataType
type: object
properties:
  companyCode:
    type: string
    required: true
    example: "FirstCoach"
  seatNumber: 
    type: number
    required: true
    example: 4
  departureLocation:
    type: string
    required: true
    example: "BEN-SG"
  destinationLocation:
    type: string
    required: true
    example: "KLG-MY"    
  message:
    type: string
    required: true
    example: "success message"